                <ref bean="kenyaemr.common.form.kpAlcoholScreening"/>
                <ref bean="kenyaemr.common.form.kpClinicalVisitForm" />
            </set>
        </property>
    </bean>
    <bean id="kenyaemr.config.report" class="org.openmrs.module.kenyacore.report.ReportConfiguration">
        <property name="commonReports">
            <set>
                <ref bean="kenyaemr.etl.common.report.moh731b" />
